{
  "expo": {
    "name": "Plantoscope",
    "slug": "Plantoscope",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/plantoscope_cropped_image.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/plantoscope_app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.plantoscope.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/plantoscope.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/plantoscope_cropped_image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "87586f6e-a953-4f4a-b072-d3cfe2a140e2"
      }
    }
  }
}
